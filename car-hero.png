<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF5F0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F0F9FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="carBody" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F7931E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Premium Sedan Car -->
  <g transform="translate(150, 200)">
    <!-- Shadow -->
    <ellipse cx="250" cy="240" rx="200" ry="20" fill="rgba(0,0,0,0.15)" filter="blur(10px)"/>
    
    <!-- Car Body -->
    <path d="M50 150 L100 80 L200 80 L250 100 L450 100 L480 140 L500 150 L500 200 L480 210 L20 210 L20 150 Z" 
          fill="url(#carBody)" stroke="#E85A2A" stroke-width="3"/>
    
    <!-- Windows -->
    <path d="M110 90 L190 90 L170 120 L130 120 Z" fill="#E8F4FD" stroke="#1a1a1a" stroke-width="2"/>
    <path d="M260 110 L440 110 L440 140 L270 140 Z" fill="#E8F4FD" stroke="#1a1a1a" stroke-width="2"/>
    
    <!-- Front Grill -->
    <rect x="20" y="165" width="20" height="30" rx="5" fill="#1a1a1a"/>
    
    <!-- Headlights -->
    <circle cx="30" cy="180" r="8" fill="#F7931E" opacity="0.9"/>
    <circle cx="490" cy="165" r="8" fill="#FFF" opacity="0.9"/>
    
    <!-- Door Lines -->
    <line x1="250" y1="140" x2="250" y2="200" stroke="#E85A2A" stroke-width="2"/>
    
    <!-- Wheels -->
    <g>
      <!-- Front Wheel -->
      <circle cx="120" cy="210" r="35" fill="#1a1a1a"/>
      <circle cx="120" cy="210" r="25" fill="#4a4a4a"/>
      <circle cx="120" cy="210" r="15" fill="#888"/>
      <circle cx="120" cy="210" r="8" fill="#ccc"/>
      
      <!-- Rear Wheel -->
      <circle cx="410" cy="210" r="35" fill="#1a1a1a"/>
      <circle cx="410" cy="210" r="25" fill="#4a4a4a"/>
      <circle cx="410" cy="210" r="15" fill="#888"/>
      <circle cx="410" cy="210" r="8" fill="#ccc"/>
    </g>
    
    <!-- Details -->
    <path d="M500 180 L490 180 L490 190 L500 190" fill="#FFF" opacity="0.3"/>
    <rect x="480" y="155" width="15" height="5" rx="2" fill="#E85A2A"/>
  </g>
  
  <!-- Decorative Elements -->
  <circle cx="100" cy="100" r="40" fill="none" stroke="#FF6B35" stroke-width="2" opacity="0.2"/>
  <circle cx="700" cy="500" r="60" fill="none" stroke="#138808" stroke-width="2" opacity="0.2"/>
</svg>
